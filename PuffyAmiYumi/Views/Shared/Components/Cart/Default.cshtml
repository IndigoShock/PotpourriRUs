@model Cart

<h4>My Cart</h4>
@foreach (CartItem item in Model.CartItems)
{
    <p>@item.ProductName</p>
    <p>@item.Price</p>
    <p>@item.Quantity</p>
}

<button asp-controller="Checkout" asp-action="Checkout" class="btn btn-outline-primary btn-lg btn-block">Proceed With Checkout</button>

<a class="btn btn-group" asp-controller="Checkout" asp-action="Checkout">Proceed To Checkout</a>